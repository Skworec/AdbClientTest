<Window x:Class="AdbClientTest.View.ConnectByIpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdbClientTest.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight"
        WindowStyle="ToolWindow"
        Title="ConnectByIpWindow">
    <StackPanel>
        <TextBox x:Name="ipTextBox" Width="200"></TextBox>
        <Button Content="Connect" Click="ButtonConnect_Click"/>
    </StackPanel>
</Window>
